{
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@api": ["src/api/index.ts"],
      "@api/*": ["src/api/*"],
      "@store": ["src/store/index.ts"],
      "@store/*": ["src/store/*"],
      "@modules/*": ["src/modules/*"],
      "@utils/*": ["src/utils/*"],
      "@app-types": ["src/types"],
      "@app-types/*": ["src/types/*"],
      "@assets/*": ["src/assets/*"]
    }
  }
}
